{
  "name": "jawira/puml-to-image",
  "description": "PlantUml wrapper to convert puml diagrams to images.",
  "type": "library",
  "require-dev": {
    "ergebnis/composer-normalize": "^2.28",
    "jawira/plantuml": "^1.2022",
    "jawira/skeleton": "^2.16"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Jawira\\PumlToImage\\": "src/"
    }
  },
  "authors": [
    {
      "name": "jawira",
      "email": "dev@tugal.be"
    }
  ],
  "require": {
    "php": "^7.4||^8.0",
    "symfony/process": "^5.4"
  },
  "config": {
    "platform-check": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    }
  }
}
